{
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer '.build/website/static/js/*.js'",
    "dev": "NODE_ENV=development node ./.config/webpack/dev.js",
    "build": "NODE_ENV=production node ./.config/webpack/build.js",
    "format": "eslint --fix workspaces --ext .ts,.tsx",
    "report-coverage": "codecov",
    "test-size": "npm i -D bundlesize && bundlesize",
    "test-unit": "NODE_ENV=test jest workspaces --config .config/jest/config.js --maxWorkers=2",
    "test-lint": "stylelint './workspaces/**/*.tsx' --config ./.config/stylelint.json",
    "cosmos": "cosmos --config ./.config/cosmos/config.js",
    "cosmos:export": "cosmos-export --config ./.config/cosmos/config.js"
  },
  "bundlesize": [
    {
      "path": ".build/**/main.*.js",
      "maxSize": "20 kB"
    },
    {
      "path": ".build/**/*.chunk.js",
      "maxSize": "50 kB"
    }
  ],
